		"@sveltejs/kit": "^2.53.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@types/node": "^22",
		"@typescript-eslint/eslint-plugin": "^8.56.0",
		"@typescript-eslint/parser": "^8.56.0",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^17.3.0",
		"mdsvex": "^0.12.6",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.5.0",
